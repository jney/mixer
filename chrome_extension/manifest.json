{
  "manifest_version": 2,
  "name": "ktulu tube",
  "version": "0.0.1",
  "description": "mixer",
  "icons": {
    "16": "images/icon48.png",
    "48": "images/icon48.png",
    "128": "images/icon48.png"
  },
  "background": {
    "persistent": false,
    "scripts": [
      "js/background.js"
    ]
  },
  "options_page": "views/options.html",
  "browser_action": {
    "default_icon": "images/icon48.png",
    "default_title": "default title",
    "default_popup": "views/popup.html"
  },
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["js/content.js"],
      "run_at": "document_start"
    }
  ],
  "permissions": [
    "activeTab",
    "tabs",
    "http://*/*",
    "https://*/*",
    "*://*/*",
    "storage",
    "declarativeContent"
  ]
}