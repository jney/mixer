{
  "manifest_version": 2,
  "name": "ktulu tube",
  "version": "0.0.1",
  "description": "mixer",
  "icons": {
    "16": "images/color16.png",
    "48": "images/color48.png",
    "128": "images/icon128.png"
  },
  "background": {
    "persistent": false,
    "scripts": [
      "js/background.js"
    ]
  },
  "options_page": "options.html",
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["js/content.js"],
      "run_at": "document_start"
    }
  ],
  "permissions": [
    "activeTab",
    "tabs",
    "http://*/*",
    "https://*/*",
    "*://*/*",
    "storage",
    "declarativeContent"
  ]
}